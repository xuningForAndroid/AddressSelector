<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="addressAnimation">
        <item name="android:windowEnterAnimation">@anim/pop_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_exit</item>
    </style>

    <style name="tableItemTextStyle">
        <item name="android:textColor">@color/colorTabText</item>
    </style>
    <style name="TabItemSelectStyle">
        <item name="android:textColor">#272727</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TabItemUnSelectStyle">
        <item name="android:textColor">#272727</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">14sp</item>
    </style>

</resources>
